# Fail2ban-jails för att skydda Traefik
[DEFAULT]
bantime = 1h
findtime = 10m
maxretry = 5
backend = auto

[traefik-http-auth]
enabled = false
# Aktivera först när ett Traefik-anpassat filter finns på plats.
port = http,https
logpath = /var/log/traefik/traefik.log

[traefik-badbots]
enabled = false
# Aktivera först när ett Traefik-anpassat filter finns på plats.
port = http,https
logpath = /var/log/traefik/access.log

